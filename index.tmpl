<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <link rel="icon" href="/favicon.ico" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Vite App</title>

  {{ with .Vite }}
  <script type="module" src="{{ . }}"></script>
  {{ end }}

  {{ with .Main }}
  <script type="module" src="{{ . }}"></script>
  {{ end }}

  {{ range .Imports }}
  <script type="module" src="{{ . }}"></script>
  {{ end }}
</head>

<body>
  <div id="app"></div>
  {{ range .Css }}
  <link rel="stylesheet" href="{{ . }}" />
  {{ end }}
</body>

</html>